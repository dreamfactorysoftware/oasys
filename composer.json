{
	"name":              "dreamfactory/oasys",
	"description":       "DreamFactory Oasys(tm) Open Authentication System",
	"keywords":          ["dreamfactory", "library", "oauth", "oauth2", "openid"],
	"homepage":          "http://dreamfactorysoftware.github.io",
	"type":              "library",
	"license":           "Apache-2.0",
	"authors":           [
		{
			"name":  "Jerry Ablan",
			"email": "jerryablan@dreamfactory.com"
		}
	],
	"minimum-stability": "dev",
	"require":           {
		"php":         ">=5.3.18",
		"ext-curl":    "*",
		"kisma/kisma": ">=0.2.13"
	},
	"require-dev":       {
		"phpunit/phpunit":              "~3.7.30@stable",
		"phpunit/phpunit-mock-objects": "@stable",
		"phpunit/phpunit-selenium":     "@stable",
		"phpunit/phpunit-story":        "@stable",
		"phpunit/dbunit":               "@stable",
		"phpunit/php-code-coverage":    "@stable",
		"phpunit/php-text-template":    "@stable",
		"phpunit/php-token-stream":     "@stable",
		"phpunit/php-timer":            "@stable",
		"phpunit/php-invoker":          "@stable"
	},
	"autoload":          {
		"psr-4": {
			"DreamFactory\\Oasys\\":        "src/"
		}
	}
}
